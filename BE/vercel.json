{
    "version": 2,
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/v1/(.*)",
            "dest": "server.js"
        },
        {
            "src": "/health",
            "dest": "server.js"
        },
        {
            "src": "/(.*)",
            "dest": "server.js",
            "methods": [
                "GET",
                "POST",
                "PUT",
                "DELETE",
                "PATCH",
                "OPTIONS"
            ]
        }
    ],
    "env": {
        "NODE_ENV": "production"
    }
}