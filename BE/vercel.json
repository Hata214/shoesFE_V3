{
    "version": 2,
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/favicon.ico",
            "dest": "https://assets.vercel.com/image/upload/v1588805858/favicon_v3.ico"
        },
        {
            "src": "/health",
            "dest": "/server.js"
        },
        {
            "src": "/api/v1/(.*)",
            "dest": "server.js",
            "methods": [
                "GET",
                "POST",
                "PUT",
                "DELETE",
                "OPTIONS"
            ],
            "headers": {
                "Access-Control-Allow-Origin": "*",
                "Access-Control-Allow-Headers": "Origin, X-Requested-With, Content-Type, Accept, Authorization",
                "Access-Control-Allow-Credentials": "true"
            }
        },
        {
            "src": "/(.*)",
            "dest": "server.js"
        }
    ],
    "env": {
        "NODE_ENV": "production"
    },
    "functions": {
        "api/v1/**": {
            "memory": 1024,
            "maxDuration": 60
        }
    }
}