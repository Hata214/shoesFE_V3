{
    "version": 2,
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/favicon.ico",
            "dest": "https://assets.vercel.com/image/upload/v1588805858/favicon_v3.ico"
        },
        {
            "src": "/health",
            "dest": "/server.js"
        },
        {
            "src": "/api/v1/(.*)",
            "dest": "/server.js"
        },
        {
            "src": "/(.*)",
            "dest": "/server.js"
        }
    ],
    "env": {
        "NODE_ENV": "production"
    },
    "functions": {
        "server.js": {
            "memory": 1024,
            "maxDuration": 10
        }
    }
}